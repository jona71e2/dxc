-image: url(/images/dxc-photo-cloud.jpg);\n  background-size: cover;\n  clip-path: polygon(0 0, 100% 0, 75% 100%, 0% 100%);\n}\n\n#partner > .image {\n  //   grid-column: 1;\n  //   grid-row: 1;\n  background-image: url(/images/Saadan-letter-DXC-din-Dynamics-365-implementering.jpg);\n  background-size: cover;\n  clip-path: polygon(25% 0, 100% 0, 100% 100%, 0% 100%);\n}\n\n// REUSABLES\n\n.triangle {\n  background-color: var.$primaryPurple;\n  width: 46px;\n  height: 46px;\n  clip-path: polygon(0 0, 0% 100%, 100% 50%);\n}\n.header-triangle {\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n}\n\nbutton {\n  display: inline-block;\n  border: none;\n  padding: 22px 50px;\n  margin: 0;\n  text-decoration: none;\n  background: var.$primaryPurple;\n  color: var.$primaryWhite;\n  cursor: pointer;\n  text-align: center;\n  transition: 0.1s;\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  font-family: dxcBold;\n  font-size: 20px;\n}\n\nbutton:hover {\n  background: var.$primaryWhite;\n  color: var.$secondaryBlack;\n  border: var.$primaryPurple solid 2px;\n}\n\nh1 {\n  //   font-size: clamp(1rem, -0.43835606 + 0.06575342vw, 4rem);\n  //   font-size: clamp(1rem, -0.875 + 8.3333vw, 3.5rem);\n  //   font-size: clamp(1rem, -0.875rem + 8.333333vw, 3.5rem);\n  font-size: clamp(1rem, -1.1212 + 9.697vw, 4rem);\n  line-height: 68px;\n  overflow: hidden;\n  white-space: nowrap;\n}\n\n//   .triangle {\n//     background-color: var.$primaryPurple;\n//     width: 46px;\n//     height: 46px;\n//     clip-path: polygon(0 0, 0% 100%, 100% 50%);\n//   }\n//   .header-triangle {\n//     display: flex;\n//     align-items: center;\n//     gap: 1rem;\n//   }\n\n@media (max-width: 1000px) {\n  #splash {\n    background-color: yellow;\n  }\n  #brochure {\n    background-color: green;\n  }\n  #partner {\n    background-color: blue;\n  }\n\n  #splash > .image {\n    height: 50vh;\n    width: 100%;\n    background-image: url(/images/Saadan-letter-DXC-din-Dynamics-365-implementering.jpg);\n    clip-path: none;\n  }\n\n  #brochure > .image {\n    height: 50vh;\n    width: 100%;\n    background-image: url(/images/dxc-photo-cloud.jpg);\n    clip-path: none;\n  }\n\n  #partner > .image {\n    height: 50vh;\n    width: 100%;\n    background-image: url(/images/Saadan-letter-DXC-din-Dynamics-365-implementering.jpg);\n    clip-path: none;\n  }\n}\n\n#blog {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n  grid-gap: 2rem;\n  margin: 1rem;\n}\n\narticle {\n  cursor: pointer;\n}\n\nimg {\n  width: 100%;\n}\n","partials/base/_vars.scss":"// COLORS //\n$primaryWhite: #fff;\n$primaryPurple: #6f2c91;\n\n$secondaryBlue: #0067b3;\n$secondaryBlack: #000;\n$secondaryLightGray: #d9d9d9;\n$secondaryDarkGray: #666;\n\n$errorText: #d0011b;\n$errorFill: lighten(#d0021b, 80%);\n\n// TEXT //\n$dxcFontRegular: \"/fonts/GT-Walsheim-Pro-Regular.woff\";\n$dxcFontBold: \"/fonts/GT-Walsheim-Pro-Bold.woff\";\n"},"lineCount":null}},"error":null,"hash":"84c67e1dd88856f4a6bebbf72c967633","cacheData":{"env":{}}}